{
  "name": "coc-neosnippet",
  "version": "1.2.2",
  "description": "neosnippet source for coc.nvim",
  "scripts": {},
  "engines": {
    "coc": ">=0.0.57"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoclide/coc-sources.git"
  },
  "keywords": [
    "coc.nvim",
    "neosnippet"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "coc.source.neosnippet.enable": {
          "type": "boolean",
          "default": true
        },
        "coc.source.neosnippet.disableSyntaxes": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          }
        },
        "coc.source.neosnippet.priority": {
          "type": "integer",
          "default": 100
        },
        "coc.source.neosnippet.shortcut": {
          "type": "string",
          "default": "NS"
        },
        "coc.source.neosnippet.filetypes": {
          "type": [
            "array",
            "null"
          ],
          "default": null,
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "author": "chemzqm@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neoclide/coc-sources/issues"
  },
  "homepage": "https://github.com/neoclide/coc-sources#readme",
  "devDependencies": {
    "coc.nvim": ">=0.0.57"
  },
  "gitHead": "ff217447719544f1224a1a0ab33a940c895ae219"
}
